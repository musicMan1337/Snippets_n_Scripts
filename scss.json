  // ************* COOL STUFF ************** //
	"Flickety Carousel": {
		"prefix": "flickety",
		"body": [
      "/* ========== Flickity Carousel ========== */",
      "",
      ".carousel {",
      "  background: black;",
      "}",
      "",
      ".carousel-cell {",
      "  margin-top: 0;",
      "  width: 70%;",
      "  height: 200px;",
      "  display: -webkit-box;",
      "  display: flex;",
      "  flex-direction: row;",
      "  justify-content: center;",
      "  align-items: center;",
      "  justify-content: space-around;",
      "}",
      "",
      ".carousel-cell > *  {",
      "  margin-top: 0;",
      "  display: block;",
      "  max-width: 100%;",
      "  max-height: 90%;",
      "  opacity: 0.7;",
      "  transform: scale(0.85);",
      "  -webkit-filter: blur(5px);",
      "  filter: blur(5px);",
      "  @include transition(opacity 0.3s, transform 0.3s, filter 0.3s);",
      "}",
      "",
      ".carousel-text {",
      "  display: block;",
      "  max-width: 35%;",
      "  color: #fff;",
      "  @include fluid-type(font-size, 320px, 1400px, 10px, 24px);",
      "}",
      "",
      ".carousel-cell.is-selected > * {",
      "  opacity: 1;",
      "  transform: scale(1);",
      "  -webkit-filter: none;",
      "  filter: none;",
      "}",
      "",
      "@media screen and (min-width: 950px) {",
      "  .carousel-cell {",
      "    height: 400px;",
      "  }",
      "}",
      "",
      "@media screen and (min-width: 1200px) {",
      "  .carousel-cell {",
      "    width: 60%;",
      "  }",
      "}",
      "",
      ".flickity-page-dots {",
      "  flex-direction: row;",
      "  justify-content: center;",
      "}",
      "",
      ".flickity-page-dots .dot {",
      "  background: red;",
      "}",
      "",
      ".flickity-prev-next-button {",
      "  width: 60px;",
      "  height: 60px;",
      "  background: transparent;",
      "  opacity: 0.6;",
      "  transform: scale(0.8);",
      "  @include transition(opacity 0.1s, transform 0.15s);",
      "}",
      "",
      ".flickity-prev-next-button:hover {",
      "  background: transparent;",
      "  opacity: 1;",
      "  transform: scale(1);",
      "}",
      "",
      ".flickity-prev-next-button .arrow {",
      "  fill: white;",
      "}",
      "",
      ".flickity-prev-next-button.no-svg {",
      "  color: white;",
      "}",
      "",
      ".flickity-prev-next-button.previous {",
      "  left: 0;",
      "}",
      "",
      ".flickity-prev-next-button.next {",
      "  right: 0;",
      "}",
      "",
      ".flickity-prev-next-button:disabled {",
      "  display: none;",
      "}",
    ],
	"description": "Flickety Carousel"
  },
