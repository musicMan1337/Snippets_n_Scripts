	"Scrollbars": {
		"prefix": "mixscrollbars",
		"body": [
      "/* ========== Custom Scrollbars ========== */",
      "",
      "@mixin scrollbars(\\$size, \\$foreground-color: #777, \\$background-color: #eee) {",
      "",
      "  ::-webkit-scrollbar {",
      "    width: \\$size;",
      "  }",
      "  ",
      "  ::-webkit-scrollbar-thumb {",
      "    background: \\$foreground-color;",
      "  }",
      "",
      "  ::-webkit-scrollbar-track {",
      "    background: \\$background-color;",
      "  }",
      "",
      "  // For Internet Explorer",
      "  body {",
      "    scrollbar-face-color: \\$foreground-color;",
      "    scrollbar-track-color: \\$background-color;",
      "  }",
      "}",
      "// @include scrollbars(.5em);",
		],
		"description": "Scrollbars"
  },
	"Buffed Scrollbars": {
		"prefix": "mixscrollbarsbuffed",
		"body": [
      "/* ========== Custom Scrollbars ==> BUFFED <== ========== */",
      "",
      "@mixin scrollbars-buffed(\\$size, \\$border-radius: 3px, \\$foreground-color: #999, \\$background-color: #eee) {",
      "",
      "  ::-webkit-scrollbar {",
      "    width: \\$size;",
      "  }",
      "",
      "  ::-webkit-scrollbar-track {",
      "    background: \\$background-color;",
      "    box-shadow: inset 0 0 6px rgba(0,0,0,0.3);",
      "    border-radius: \\$border-radius;",
      "  }",
      "",
      "  ::-webkit-scrollbar-thumb {",
      "    border-radius: \\$border-radius;",
      "    background: \\$foreground-color; ",
      "    box-shadow: inset 0 0 6px rgba(0,0,0,0.5);",
      "  }",
      "  ::-webkit-scrollbar-thumb:window-inactive {",
      "    background-color: mix(\\$foreground-color, white, 40%);",
      "  }",
      "",
      "  // For Internet Explorer",
      "  body {",
      "    scrollbar-face-color: \\$foreground-color;",
      "    scrollbar-track-color: \\$background-color;",
      "  }",
      "}",
      "// @include scrollbars-buffed(.5em);",
		],
		"description": "Buffed Scrollbars"
  },
  "Fluid Element Sizing": {
    "prefix": "mixfluid",
    "body": [
      "/* ========== Fluid Element Sizing (px/rem) ========== */",
      "",
      "@mixin fluid-type(\\$properties, \\$min-vw, \\$max-vw, \\$min-value, \\$max-value) {",
      "  @each \\$property in \\$properties {",
      "    #{\\$property}: \\$min-value;",
      "  }",
      "",
      "  @media (min-width: \\$min-vw) {",
      "    @each \\$property in \\$properties {",
      "      #{\\$property}: calc(#{\\$min-value} + #{strip-unit(\\$max-value - \\$min-value)} * (100vw - #{\\$min-vw}) / #{strip-unit(\\$max-vw - \\$min-vw)});",
      "    }",
      "  }",
      "",
      "  @media (min-width: \\$max-vw) {",
      "    @each \\$property in \\$properties {",
      "      #{\\$property}: \\$max-value;",
      "    }",
      "  }",
      "}",
      "",
      "@function strip-unit(\\$number) {",
      "  @if type-of(\\$number) == \"number\" and not unitless(\\$number) {",
      "    @return \\$number / (\\$number * 0 + 1);",
      "  }",
      "  @return \\$number;",
      "}",
      "",
      "// Single property",
      "// @include fluid-type(font-size, 320px, 1400px, 16px, 24px);",
      "",
      "// Multiple properties with same values (avoid \"em\")",
      "//   @include fluid-type(padding-bottom padding-top, 20rem, 70rem, 2rem, 4rem);",
    ],
    "description": "Fluid Element Sizing"
  },
  "Color Themes": {
    "prefix": "mixcolorthemes",
    "body": [
      "/* ========== Color Themes ========== */",
      "",
      "\\$themes: (",
      "  summer: (",
      "    primary-color: #ffd700,",
      "    secondary-color: #0084b1,",
      "  ),",
      "  winter: (",
      "    primary-color: #7292b5,",
      "    secondary-color: #f0f8ff,",
      "  ),",
      ");",
      "",
      "@mixin theme(\\$properties, \\$key) {",
      "  @each \\$theme in map-keys(\\$themes) {",
      "    .#{\\$theme} & {",
      "      @each \\$property in \\$properties {",
      "        #{\\$property}: map-deep-get(\\$themes, \\$theme, \\$key);",
      "      }",
      "    }",
      "  }",
      "}",
      "",
      "@function map-deep-get(\\$map, \\$keys...) {",
      "  @each \\$key in \\$keys {",
      "    \\$map: map-get(\\$map, \\$key);",
      "  }",
      "  @return \\$map;",
      "}",
      "// Use:",
      "// @include theme(border-bottom border-top, secondary-color);",
    ],
    "description": "Color Themes"
  },
	"Transitions": {
    "prefix": "mixtransitions",
		"body": [
      "/* ========== Transitions ========== */",
      "",
      "@mixin transition(\\$transitions...) {",
      "  \\$unfoldedTransitions: ();",
      "  @each \\$transition in \\$transitions {",
      "    \\$unfoldedTransitions: append(\\$unfoldedTransitions, unfoldTransition(\\$transition), comma);",
      "  }",
      "  ",
      "  transition: \\$unfoldedTransitions;",
      "}",
      "",
      "@function unfoldTransition (\\$transition) {",
      "  // Default values",
      "  \\$property: all;",
      "  \\$duration: .2s;",
      "  \\$easing: null;",
      "  \\$delay: null;",
      "  \\$defaultProperties: (\\$property, \\$duration, \\$easing, \\$delay);",
      "",
      "  // Grab transition properties if they exist",
      "  \\$unfoldedTransition: ();",
      "  @for \\$i from 1 through length(\\$defaultProperties) {",
      "    \\$p: null;",
      "    @if \\$i <= length(\\$transition) {",
      "      \\$p: nth(\\$transition, \\$i)",
      "    } @else {",
      "      \\$p: nth(\\$defaultProperties, \\$i)",
      "    }",
      "    \\$unfoldedTransition: append(\\$unfoldedTransition, \\$p);",
      "  }",
      "",
      "  @return \\$unfoldedTransition;",
      "}",
      "// Use:",
      "// @include transition(opacity 0.3s, transform 0.3s, filter 0.3s ease-in);",
		],
		"description": "Transitions"
  },
